# ============================================
# TEMPLATE FOR SENSITIVE CREDENTIALS
# ============================================
# Copy this file to application-secrets.properties and fill in your values
# DO NOT commit application-secrets.properties to git!

# PostgreSQL Database Credentials
db.url=jdbc:postgresql://localhost:5432/document_organiser
db.username=your_database_username
db.password=your_database_password

# JWT Secrets (must be at least 256 bits / 32 characters)
jwt.secret=your-jwt-secret-key-must-be-at-least-256-bits-long-for-security

# Google OAuth2 Credentials
# Get these from: https://console.cloud.google.com/apis/credentials
google.client-id=your-google-client-id
google.client-secret=your-google-client-secret

# MinIO/S3 Storage Credentials
storage.access-key=your-storage-access-key
storage.secret-key=your-storage-secret-key

# Redis Password (if applicable)
redis.password=your-redis-password

